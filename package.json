    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.25",
    "@types/react": "^18.2.0",
    "@types/react-bootstrap": "^0.32.32",
    "@types/react-dom": "^18.2.1",
    "bootstrap": "^5.2.3",
    "moment-timezone": "^0.5.43",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-moment": "^1.1.3",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
